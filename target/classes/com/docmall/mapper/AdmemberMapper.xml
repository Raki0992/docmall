<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.docmall.mapper.AdmemberMapper">
	
	<select id="member_list" parameterType="String" resultType="com.docmall.domain.MemberVO">
		SELECT
			mbsp_id, MBSP_NAME, MBSP_EMAIL, MBSP_ADDR, MBSP_PHONE, MBSP_LASTLOGIN
		FROM
			MBSP_TBL
	</select>
	
	<delete id="member_delete" parameterType="String">
		DELETE FROM
			MBSP_TBL
		WHERE
			MBSP_ID = #{mbsp_id}
	</delete>
	
	</mapper>